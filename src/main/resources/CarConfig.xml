<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="car" class="com.caps.demo.spring.xml.Car" >
		<property name="name" value="Nano"/>
		<property name="color" value="pink"/>
		<property name="price" value="10000000"/>
		<property name="engine" ref="engine" />
	</bean>

	<bean id="engine" class="com.caps.demo.spring.xml.Engine">
		<property name="type" value="Snapdragon V8 Phantom"/>
		<property name="model" value="1947 Classic"></property>
	</bean>

</beans>